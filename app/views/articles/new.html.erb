<h1>New article</h1>

<%= form_with model: @article do |f| %>
  <%= f.label :title %>
  <br>
  <%= f.text_field :title, class:"form-control" %>
  <br>
  <%= f.label :content %>
  <br>
  <%= f.text_field :content, class:"form-control" %>
  <br>
  <%= f.submit class:"btn btn-primary" %>
  <% end %>
  <%= link_to "Back", articles_path, class:"btn btn-outline-primary" %>
